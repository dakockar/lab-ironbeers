{{#each randomBeer}}
<div id="random-container" class="wrapper">
    <div>
        <img id="random-img" src="{{this.image_url}}" alt="">
    </div>

    <div class="box">
        {{> beerPartial this}}
        <p>
            {{this.food_pairing}}
        </p>
        <p>
            {{this.brewers_tips}}
        </p>
    </div>
</div>
{{/each}}